    "tm:required": [
        "#/properties/heartrate_heartrate"
    ],
    "properties": {
        "heartrate_heartrate": {
            "sdf:jsonPointer": "#/sdfObject/heartrate/sdfProperty/heartrate",
            "description": "This Property describes the heart rate in bpm.",
            "readOnly": true,
            "type": "integer",
            "minimum": 0
        },
        "heartrate_range": {
            "sdf:jsonPointer": "#/sdfObject/heartrate/sdfProperty/range",
            "description": "The valid range for the Property in the Resource as an integer. The first value in the array is the minimum value, the second value in the array is the maximum value.",
            "readOnly": true,
            "type": "array",
            "minItems": 2,
            "maxItems": 2,
            "items": {
                "type": "integer"
            }
        },
        "heartrate_step": {
            "sdf:jsonPointer": "#/sdfObject/heartrate/sdfProperty/step",
            "description": "Step value across the defined range when the range is an integer.  This is the increment for valid values across the range; so if range is 0..10 and step is 2 then valid values are 0,2,4,6,8,10.",
            "readOnly": true,
            "type": "integer"
        }
    }
